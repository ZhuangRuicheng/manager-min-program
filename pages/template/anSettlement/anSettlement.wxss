/* pages/template/anSettlement/anSettlement.wxss */
page{
  background-color: #f7f7f7;
}
.an{
  position: fixed;
  width: 100%;
  z-index: 9999999;
}
/* .all{
padding-top: 100rpx;
} */
.an-header{
  height: 100rpx;
  line-height: 100rpx;
  color: #333333;
  font-size: 30rpx;
  background-color: white;
  display: flex;
  padding-left: 30rpx;
  margin-bottom: 15rpx;
}
.view-btn{
  width: 180rpx;
  text-align: center;
  position: absolute;
  right: 30rpx;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: white;
  background-color: #ea5415;
  border-radius: 12rpx;
  margin-top: 20rpx;
}
.an-content{
  
  height: 100rpx;
  line-height: 100rpx;
  background-color: white;
  padding-left: 30rpx;
  border-bottom: 1rpx solid #efefef;
  display: flex;
}
.day-time{
  font-size: 34rpx;
  color: #333333;
}
.month-time{
  font-size: 30rpx;
  color: #999999;
}
.incomes{
  position: absolute;
  right: 30rpx;
}
.an-sign{
  font-size: 24rpx;
  color: #999999;
}
.an-money{
  font-size: 34rpx;
  color: #333333;
  font-weight: bold;
}