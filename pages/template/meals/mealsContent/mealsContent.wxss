/* pages/template/meals/mealsContent/mealsContent.wxss */

page {
  background-color: #f7f7f7;
}

.header {
  height: 100rpx;
  background-color: white;
  margin-bottom: 30rpx;
}

.picker {
  border: 1rpx #ccc solid;
  font-size: 30rpx;
  width: 235rpx;
  height: 55rpx;
  margin: 15rpx 0rpx 0rpx 30rpx;
}

.text {
  color: 30rpx;
  margin: 8rpx 10rpx 0rpx 20rpx;
}

.lower {
  width: 29rpx;
  height: 16rpx;
  position: absolute;
  left: 215rpx;
  top: 40rpx;
}

.section {
  display: flex;
}

.right {
  width: 193rpx;
  height: 50rpx;
  font-size: 30rpx;
  border: 1rpx #ccc solid;
  position: absolute;
  top: 18rpx;
  right: 30rpx;
  text-align: center;
  padding-top: 6rpx;
}

.googs-content {
  height: 204rpx;
  background-color: white;
  border-bottom: 1rpx #ccc solid;
  display: flex;
}

.img {
  width: 150rpx;
  height: 150rpx;
  margin: 30rpx 30rpx 24rpx 30rpx;
}

.goods-price {
  display: flex;
  margin-top: 40rpx;
}

.goods-name {
  font-size: 32rpx;
  color: #333;
}

.price {
  font-size: 34rpx;
  color: red;
  font-weight: bold;
  position: absolute;
  right: 50rpx;
}

.state {
  font-size: 30rpx;
  color: green;
  margin-top: 50rpx;
}
.dwon{
  font-size: 30rpx;
  color: red;
  margin-top: 50rpx;
}
.buydes-dialog-container {
  width: 100%;
  height: 100%;
  justify-content: space-between;
  background-color: rgba(15, 15, 26, 0.7);
  position: fixed;
  z-index: 999;
  /* position: absolute; */
  top: 0rpx;
}

.buydes-dialog-container-top {
  flex-grow: 1;
}

.buydes-dialog-container-bottom {
  display: flex;
  flex-grow: 0;
}

.popup {
  margin: 100rpx 0rpx 80rpx 60rpx;
  background-color: white;
}

.ticketShow {
  padding-bottom: 30rpx;
  /* height: 450rpx; */
  width: 640rpx;
  border-bottom: 1rpx #dfdfdf dotted;
}

.cancel {
  position: absolute;
  right: 64rpx;
  margin-top: 20rpx;
}

.hotelName {
  font-size: 38rpx;
  width: 640rpx;
  height: 30rpx;
  text-align: center;
  padding-top: 50rpx;
  color: #333;
  margin-bottom: 30rpx;
  /* margin-bottom: 60rpx; */
}

.for-switch {
  font-size: 32rpx;
  color: #333;
  margin-left: 30rpx;
}

.switch {
  padding-top: 30rpx;
  border-top: 1rpx solid #dfdfdf;
}

switch {
  transform: scale(0.9);
  position: absolute;
  right: 80rpx;
}

input {
  width: 400rpx;
  border: 1rpx solid #ccc;
  height: 50rpx;
  font-size: 30rpx;
  padding-left: 10rpx;
}

.input {
  margin-left: 30rpx;
}

.food-name {
  margin-left: 30rpx;
  margin-top: 50rpx;
  font-size: 32rpx;
  display: flex;
  color: #333;
}

.food-price {
  margin-top: 30rpx;
}

.for-food-price {
  margin-left: 60rpx;
}

.add-picker-content {
  display: flex;
  font-size: 32rpx;
  color: #333;
  margin-top: 30rpx;
}

.for-food-classify {
  margin-left: 90rpx;
}

.model-picker {
  margin-left: 30rpx;
  width: 400rpx;
  border: 1rpx solid #ccc;
  height: 50rpx;
  font-size: 30rpx;
  color: #333;
  line-height: 50rpx;
  padding-left: 10rpx;
  display: flex;
}

.sign {
  width: 29rpx;
  height: 16rpx;
}

.sign-location {
  position: absolute;
  right: 115rpx;
}

.describe {
  margin-top: 30rpx;
  display: flex;
  font-size: 32rpx;
  color: #333;
  margin-left: 30rpx;
}

textarea {
  height: 120rpx;
  width: 400rpx;
  border: 1rpx #ccc solid;
  font-size: 26rpx;
  padding-left: 10rpx;
  padding-top: 10rpx;
}

.up-img {
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  width: 250rpx;
  border: 1rpx #ccc solid;
  font-size: 30rpx;
  color: #333;
}

.up-show {
  height: 120rpx;
  width: 250rpx;
}
.showBtn{
  width: 360rpx;
  height: 80rpx;
  text-align: center;
  background-color: #ea5415;
  color: white;
  line-height: 80rpx;
  margin-top: 50rpx;
}
